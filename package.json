{
  "name": "express-babel-7",
  "version": "0.0.1",
  "engines": {
    "node": "10.15.x"
  },
  "description": "nodejs + express 4 + babel 7 boilerplate project",
  "main": "server.js",
  "scripts": {
    "lint": "eslint",
    "dev": "nodemon --exec babel-node src/server/index.js",
    "build": "rm -rf dist && babel src --out-dir dist",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.16",
    "mongoose-bcrypt": "^1.8.0",
    "mongoose-string-query": "^0.2.7",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.1",
    "pm2": "^4.4.0",
    "uuid": "^8.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-root-import": "^6.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2"
  },
  "repository": "git@github.com:liudonghua123/express-babel-7.git",
  "author": "Donghua Liu <liudonghua123@gmail.com>",
  "license": "MIT"
}
